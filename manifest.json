{
    "name": "THE EXTENSION",
    "description": "When creating a new tab it makes \"Google\" more visual and has a weird image background",
    "version": "1.3",
    "manifest_version": 2,
    "chrome_url_overrides": {"newtab": "darktab.html"},
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["index.js"]
        }
    ],
    "browser_action":{
        "default_popup": "darktab.html",
        "default_title": "Tab"
    }
}    